{
  "name": "chessagon",
  "version": "0.0.1",
  "scripts": {
    "wasm-once": "wasm-pack build --dev ../engine --out-dir ../site/.wasm-engine",
    "wasm-once-prod": "wasm-pack build ../engine --out-dir ../site/.wasm-engine",
    "wasm": "find ../engine/ -name '*.rs' | entr pnpm wasm-once",
    "db-migrate": "pnpm exec drizzle-kit generate:sqlite && pnpm exec drizzle-kit push:sqlite",
    "db-push-prod": "pnpm exec drizzle-kit push:sqlite -- --prod",
    "dev": "vite dev",
    "build": "vite build",
    "package": "svelte-kit package",
    "preview": "vite preview",
    "test": "playwright test",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write  --plugin prettier-plugin-svelte ."
  },
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "@skeletonlabs/skeleton": "^2.6.0",
    "@skeletonlabs/tw-plugin": "^0.3.0",
    "@sveltejs/adapter-vercel": "^3.1.0",
    "@sveltejs/kit": "1.27.7",
    "@tailwindcss/forms": "^0.5.7",
    "@types/better-sqlite3": "^7.6.8",
    "@types/node": "^20.10.4",
    "autoprefixer": "^10.4.16",
    "better-sqlite3": "^9.2.2",
    "dotenv": "^16.3.1",
    "drizzle-kit": "^0.20.6",
    "postcss": "^8.4.32",
    "postcss-load-config": "^5.0.2",
    "prettier-plugin-svelte": "^3.1.2",
    "svelte": "^4.2.8",
    "svelte-check": "^3.6.2",
    "svelte-preprocess": "^5.1.1",
    "tailwind-scrollbar-utilities": "^0.2.0",
    "tailwindcss": "^3.3.6",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^4.5.0",
    "vite-plugin-favicon2": "^1.1.5",
    "vite-plugin-top-level-await": "^1.3.1",
    "vite-plugin-wasm": "^3.2.2"
  },
  "type": "module",
  "dependencies": {
    "@libsql/client": "0.4.0-pre.5",
    "@lucia-auth/adapter-sqlite": "^2.0.1",
    "@paralleldrive/cuid2": "^2.2.2",
    "@sveltejs/adapter-auto": "^2.1.1",
    "drizzle-orm": "^0.29.1",
    "lucia": "^2.7.4",
    "pusher": "^5.2.0",
    "pusher-js": "8.3.0",
    "ts-todo": "^2.0.0",
    "ts-unwrap": "^2.0.0",
    "zod": "^3.22.4",
    "zod-form-data": "^2.0.2"
  }
}
