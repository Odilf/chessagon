{
  "name": "chessagon",
  "version": "0.0.1",
  "scripts": {
    "wasm-once": "wasm-pack build ../engine --out-dir ../site/.wasm-engine",
    "wasm": "find ../engine/ -name '*.rs' | entr pnpm wasm-once",
    "dev": "vite dev",
    "gen-types": "supabase gen types typescript --local > ./src/lib/db/generatedTypes.ts",
    "build": "vite build",
    "package": "svelte-kit package",
    "preview": "vite preview",
    "test": "playwright test",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write  --plugin prettier-plugin-svelte ."
  },
  "devDependencies": {
    "@playwright/test": "^1.38.1",
    "@skeletonlabs/skeleton": "^2.2.0",
    "@skeletonlabs/tw-plugin": "^0.2.1",
    "@sveltejs/adapter-auto": "2.1.0",
    "@sveltejs/kit": "1.25.1",
    "@tailwindcss/forms": "^0.5.6",
    "@types/node": "^20.7.1",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.24",
    "postcss-load-config": "^4.0.1",
    "prettier-plugin-svelte": "^3.0.3",
    "supabase": "^1.99.5",
    "svelte": "^4.2.1",
    "svelte-check": "^3.5.2",
    "svelte-preprocess": "^5.0.4",
    "tailwindcss": "^3.3.2",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-favicon2": "^1.1.5",
    "vite-plugin-top-level-await": "^1.3.1",
    "vite-plugin-wasm": "^3.2.2"
  },
  "type": "module",
  "dependencies": {
    "@supabase/auth-helpers-sveltekit": "^0.10.3",
    "@supabase/supabase-js": "^2.36.0",
    "gun": "^0.2020.1239",
    "peerjs": "^1.5.1",
    "uid": "^2.0.2",
    "webrtc-adapter": "^8.2.3"
  }
}
