{
  "name": "chessagon",
  "version": "0.0.1",
  "scripts": {
    "wasm-once": "wasm-pack build --dev ../engine --out-dir ../site/.wasm-engine",
    "wasm": "find ../engine/ -name '*.rs' | entr pnpm wasm-once",
    "db-migrate": "pnpm exec drizzle-kit generate:sqlite && pnpm exec drizzle-kit push:sqlite",
    "dev": "vite dev",
    "build": "vite build",
    "package": "svelte-kit package",
    "preview": "vite preview",
    "test": "playwright test",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write  --plugin prettier-plugin-svelte ."
  },
  "devDependencies": {
    "@playwright/test": "^1.39.0",
    "@skeletonlabs/skeleton": "^2.4.0",
    "@skeletonlabs/tw-plugin": "^0.2.3",
    "@sveltejs/adapter-auto": "2.1.1",
    "@sveltejs/kit": "1.27.3",
    "@tailwindcss/forms": "^0.5.6",
    "@types/better-sqlite3": "^7.6.6",
    "@types/node": "^20.8.10",
    "autoprefixer": "^10.4.16",
    "better-sqlite3": "^9.0.0",
    "dotenv": "^16.3.1",
    "drizzle-kit": "^0.19.13",
    "postcss": "^8.4.31",
    "postcss-load-config": "^4.0.1",
    "prettier-plugin-svelte": "^3.0.3",
    "svelte": "^4.2.2",
    "svelte-check": "^3.5.2",
    "svelte-preprocess": "^5.0.4",
    "tailwind-scrollbar-utilities": "^0.2.0",
    "tailwindcss": "^3.3.5",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-favicon2": "^1.1.5",
    "vite-plugin-top-level-await": "^1.3.1",
    "vite-plugin-wasm": "^3.2.2"
  },
  "type": "module",
  "dependencies": {
    "@libsql/client": "^0.3.6",
    "@lucia-auth/adapter-sqlite": "^2.0.1",
    "@paralleldrive/cuid2": "^2.2.2",
    "drizzle-orm": "^0.28.6",
    "lucia": "^2.7.4",
    "pusher": "^5.1.3",
    "pusher-js": "^8.3.0",
    "ts-todo": "^2.0.0",
    "ts-unwrap": "^2.0.0",
    "zod": "^3.22.4",
    "zod-form-data": "^2.0.2"
  }
}
